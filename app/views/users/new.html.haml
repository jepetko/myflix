%section.register.container
  .row
    .col-sm-10.col-sm-offset-1
      = bootstrap_form_for(@user) do |f|
        %header
          %h1 Register
        = f.text_field :email
        = f.password_field :password
        = f.password_field :password_confirmation
        = f.text_field :full_name
        = f.submit 'Sign up'